    <!-- brand -->
    <div class="brand">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="titlepage">
                        <h2>Our Products</h2><br/>
                        <h3>Search your desired product</h3>
                        <hr / >
                        <%= form_with url: "/gallery/search", local: true,method: :post do |form| %>
                          <%= form.label :query, "Search for:" %>
                          <%= form.text_field :query %>
                          <%= form.submit "Search" %>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
        <div class="brand-bg">
            <div class="container">
